        <items xsi:type="form:FormField">
          <name>FunctionalOptionUseOrderWarehouse</name>
          <id>130</id>
          <title>
            <key>en</key>
            <value>Use two-phase warehouse</value>
          </title>
          <title>
            <key>ru</key>
            <value>Использовать ордерный склад</value>
          </title>
          <title>
            <key>vi</key>
            <value>Sử dụng hóa đơn không kiêm phiếu xuất nhập kho</value>
          </title>
          <visible>true</visible>
          <enabled>true</enabled>
          <userVisible>
            <common>true</common>
