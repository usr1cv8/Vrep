    <main>true</main>
    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>SELECT
	InformationRegisterDataAreas.DataAreaAuxiliaryData,
	InformationRegisterDataAreas.Status,
	InformationRegisterDataAreas.Repeat,
	InformationRegisterDataAreas.ExportID,
	InformationRegisterDataAreas.ProcessingError,
	InformationRegisterDataAreas.Variant,
	DataAreaPresentation.Value AS Presentation,
	DataAreaTimeZone.Value AS TimeZone,
	DataAreasPrefix.Value AS Prefix
FROM
	InformationRegister.DataAreas AS InformationRegisterDataAreas
		LEFT JOIN Constant.DataAreaPresentation AS DataAreaPresentation
		ON InformationRegisterDataAreas.DataAreaAuxiliaryData = DataAreaPresentation.DataAreaAuxiliaryData
		LEFT JOIN Constant.DataAreaTimeZone AS DataAreaTimeZone
		ON InformationRegisterDataAreas.DataAreaAuxiliaryData = DataAreaTimeZone.DataAreaAuxiliaryData
		LEFT JOIN Constant.DataAreasPrefix AS DataAreasPrefix
		ON InformationRegisterDataAreas.DataAreaAuxiliaryData = DataAreasPrefix.DataAreaAuxiliaryData</queryText>
      <mainTable>InformationRegister.DataAreas</mainTable>
      <dynamicDataRead>true</dynamicDataRead>
      <autoFillAvailableFields>true</autoFillAvailableFields>
      <customQuery>true</customQuery>
      <autoSaveUserSettings>true</autoSaveUserSettings>
      <getInvisibleFieldPresentations>true</getInvisibleFieldPresentations>
