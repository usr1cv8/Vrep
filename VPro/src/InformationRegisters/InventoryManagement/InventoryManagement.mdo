  <resources uuid="19770c7f-7089-4ad4-8fbc-27bc115dae14">
    <name>InventoryMinimumLevel</name>
    <synonym>
      <key>en</key>
      <value>Minimum inventory level</value>
    </synonym>
    <synonym>
      <key>ru</key>
      <value>Минимальный уровень запаса</value>
