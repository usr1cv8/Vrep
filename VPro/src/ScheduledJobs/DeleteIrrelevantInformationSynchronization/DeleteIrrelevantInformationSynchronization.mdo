  <name>DeleteIrrelevantInformationSynchronization</name>
  <synonym>
    <key>en</key>
    <value>Delete outdated synchronization information</value>
  </synonym>
  <synonym>
    <key>ru</key>
    <value>Удаление неактуальной информации синхронизации</value>
  </synonym>
  <synonym>
