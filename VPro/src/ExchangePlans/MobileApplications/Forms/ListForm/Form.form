    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>SELECT
	CASE
		WHEN ПланОбменаМобильноеПриложение.DeletionMark
			THEN 1
		ELSE 0
	END AS ВариантКартинки,
	ПланОбменаМобильноеПриложение.Ref,
	ПланОбменаМобильноеПриложение.DeletionMark,
	ПланОбменаМобильноеПриложение.ThisNode,
	ПланОбменаМобильноеПриложение.Code,
	ПланОбменаМобильноеПриложение.Description,
	ПланОбменаМобильноеПриложение.SentNo,
	ПланОбменаМобильноеПриложение.ReceivedNo,
	ПланОбменаМобильноеПриложение.AccessIsDenied
FROM
	ExchangePlan.MobileApplications AS ПланОбменаМобильноеПриложение
WHERE
	NOT ПланОбменаМобильноеПриложение.ThisNode</queryText>
      <mainTable>ExchangePlan.MobileApplications</mainTable>
      <dynamicDataRead>true</dynamicDataRead>
      <autoFillAvailableFields>true</autoFillAvailableFields>
