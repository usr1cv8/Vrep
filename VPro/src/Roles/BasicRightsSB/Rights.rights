   StrContains(",Condition,,RightsSettings,Object, ,ReadRight,ChangeRight,", ",#Parameter(12),")
  StrContains(&amp;AccessKindsWithDisabledUse, ",#Parameter(12),"))
  ("#Parameter(14)" = ""  "#Parameter(15)" = ""
