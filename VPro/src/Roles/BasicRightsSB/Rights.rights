   StrContains(",Condition,,RightsSettings,Object, ,ReadRight,ChangeRight,", ",#Parameter(8),")
  StrContains(&amp;AccessKindsWithDisabledUse, ",#Parameter(8),"))
  ("#Parameter(10)" = ""  "#Parameter(11)" = ""
