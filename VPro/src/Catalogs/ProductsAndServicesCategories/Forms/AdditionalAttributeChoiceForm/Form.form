    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>SELECT
	Properties.Ref,
	Properties.DataVersion,
	Properties.DeletionMark,
	Properties.Predefined,
	Properties.Title,
	CASE
		WHEN Properties.PropertySet = VALUE(Catalog.AdditionalAttributesAndInformationSets.EmptyRef)
			THEN &amp;PresentationGroupingOfCommonProperties
		WHEN Properties.PropertySet.Parent = VALUE(Catalog.AdditionalAttributesAndInformationSets.EmptyRef)
			THEN Properties.PropertySet
		ELSE Properties.PropertySet.Parent.Description + &quot; (&quot; + Properties.PropertySet.Description + &quot;)&quot;
	END AS PropertySetGrouping,
	Properties.PropertySet,
	Properties.AdditionalValuesAreUsed,
	Properties.AdditionalValuesOwner,
	Properties.Description,
	Properties.ValueType,
	Properties.ThisIsAdditionalInformation,
	Properties.MultilineTextBox,
	Properties.FillObligatory,
	Properties.ValueFormHeader,
	Properties.ValueChoiceFormHeader,
	Properties.FormatProperties,
	Properties.Comment,
	Properties.ToolTip,
	CASE
		WHEN Properties.ThisIsAdditionalInformation
			THEN 2
		ELSE 1
	END + CASE
		WHEN Properties.Predefined
			THEN 6
		WHEN Properties.DeletionMark
			THEN 3
		ELSE 0
	END AS PictureNumber
FROM
	ChartOfCharacteristicTypes.AdditionalAttributesAndInformation AS Properties</queryText>
