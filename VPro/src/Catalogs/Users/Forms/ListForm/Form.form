    <title>
      <key>ru</key>
      <value>Пользователи список</value>
    </title>
    <title>
      <key>vi</key>
      <value>Người sử dụng danh sách</value>
    </title>
    <id>1</id>
    <valueType>
      <types>DynamicList</types>
    </valueType>
    <view>
      <common>true</common>
    </view>
    <edit>
      <common>true</common>
    </edit>
    <notDefaultUseAlwaysAttributes xsi:type="form:DataPath">
      <segments>UsersList.NotValid</segments>
    </notDefaultUseAlwaysAttributes>
    <notDefaultUseAlwaysAttributes xsi:type="form:DataPath">
      <segments>UsersList.Ref</segments>
    </notDefaultUseAlwaysAttributes>
    <main>true</main>
    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>SELECT
	Users.Ref,
	Users.Prepared,
	Users.DeletionMark,
	Users.NotValid,
	Users.Description,
	Users.Service,
	Users.Comment,
	CASE
		WHEN Users.DeletionMark
			THEN 0
		ELSE 1
	END AS PictureNumber
FROM
	Catalog.Users AS Users
WHERE
	(&amp;SelectHierarchy
				AND TRUE In
					(SELECT
						TRUE
					FROM
						InformationRegister.UsersGroupsContents AS UsersGroupsContents
					WHERE
						UsersGroupsContents.User = Users.Ref
						AND UsersGroupsContents.UsersGroup = &amp;UsersGroup)
			OR Not &amp;SelectHierarchy
				AND TRUE In
					(SELECT
						TRUE
					FROM
						Catalog.UsersGroups.Content AS UsersGroupsContents
					WHERE
						UsersGroupsContents.User = Users.Ref
							AND UsersGroupsContents.Ref = &amp;UsersGroup))</queryText>
      <mainTable>Catalog.Users</mainTable>
      <dynamicDataRead>true</dynamicDataRead>
      <autoFillAvailableFields>true</autoFillAvailableFields>
      <customQuery>true</customQuery>
      <autoSaveUserSettings>true</autoSaveUserSettings>
      <getInvisibleFieldPresentations>true</getInvisibleFieldPresentations>
    </extInfo>
