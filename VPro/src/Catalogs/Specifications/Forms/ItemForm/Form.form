    <title>
      <key>en</key>
      <value>Import data from an external source</value>
    </title>
    <title>
      <key>ru</key>
      <value>Загрузка данных из внешнего источника</value>
    </title>
    <title>
      <key>vi</key>
      <value>Kết nhập dữ liệu từ nguồn ngoài</value>
    </title>
    <id>1</id>
    <toolTip>
      <key>en</key>
      <value>Import data from an external source</value>
    </toolTip>
    <toolTip>
      <key>ru</key>
      <value>Загрузка данных из внешнего источника</value>
    </toolTip>
    <toolTip>
      <key>vi</key>
      <value>Kết nhập dữ liệu từ nguồn ngoài</value>
    </toolTip>
    <use>
      <for>
        <value>true</value>
        <role>Role.FullRights</role>
      </for>
      <for>
        <value>true</value>
        <role>Role.UseDataImportFromExternalSources</role>
      </for>
    </use>
    <picture xsi:type="core:PictureRef">
      <picture>CommonPicture.LoadFromFile</picture>
    </picture>
    <action xsi:type="form:FormCommandHandlerContainer">
      <handler>
        <name>DataImportFromExternalSources</name>
      </handler>
    </action>
    <currentRowUse>DontUse</currentRowUse>
