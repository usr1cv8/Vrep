    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>SELECT
	Files.Ref,
	Files.DeletionMark,
	Files.Predefined,
	Files.Code,
	Files.Description,
	Files.Author,
	Files.CreationDate,
	Files.StoreVersions,
	Files.IsEditing,
	Files.CurrentVersion,
	Files.PictureIndex,
