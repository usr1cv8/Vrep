	CatalogNonCurrentAssets.DeletionMark AS DeletionMark,
	CatalogNonCurrentAssets.Predefined AS Predefined,
	CatalogNonCurrentAssets.Code AS Code,
	CatalogNonCurrentAssets.Description AS Description,
	CatalogNonCurrentAssets.Type AS Type,
	CatalogNonCurrentAssets.GLAccount AS GLAccount,
	CatalogNonCurrentAssets.DepreciationAccount AS DepreciationAccount,
	CatalogNonCurrentAssets.DepreciationMethod AS DepreciationMethod,
	CatalogNonCurrentAssets.InitialCost AS InitialCost,
	CatalogNonCurrentAssets.MeasurementUnit AS MeasurementUnit,
	FixedAssetsBalance.CostBalance AS CostBalance,
	FixedAssetsBalance.DepreciationBalance AS DepreciationBalance,
	PRESENTATION(ISNULL(FixedAssetsStatesSliceLast.State, &amp;NotAcceptedForAccounting)) AS State,
	FixedAssetsBalance.CostBalance - FixedAssetsBalance.DepreciationBalance AS DepreciatedCost,
	FixedAssetsParametersSliceLast.Recorder.Company AS Company
FROM
	Catalog.FixedAssets AS CatalogNonCurrentAssets
		LEFT JOIN InformationRegister.FixedAssetsStates.SliceLast AS FixedAssetsStatesSliceLast
		ON CatalogNonCurrentAssets.Ref = FixedAssetsStatesSliceLast.FixedAsset
		LEFT JOIN AccumulationRegister.FixedAssets.Balance AS FixedAssetsBalance
		ON CatalogNonCurrentAssets.Ref = FixedAssetsBalance.FixedAsset
		LEFT JOIN InformationRegister.FixedAssetsParameters.SliceLast AS FixedAssetsParametersSliceLast
		ON CatalogNonCurrentAssets.Ref = FixedAssetsParametersSliceLast.FixedAsset</queryText>
