    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>
SELECT ALLOWED
	ProductsAndServicesCharacteristics.Ref AS Ref,
	ProductsAndServicesCharacteristics.DataVersion AS DataVersion,
	ProductsAndServicesCharacteristics.DeletionMark AS DeletionMark,
	ProductsAndServicesCharacteristics.Owner AS Owner,
	ProductsAndServicesCharacteristics.Code AS Code,
	ProductsAndServicesCharacteristics.Description AS Description,
	ProductsAndServicesCharacteristics.Predefined AS Predefined,
	ProductsAndServicesCharacteristics.PredefinedDataName AS PredefinedDataName,
	ProductsAndServicesCharacteristics.Presentation AS Presentation,
	CASE
		WHEN VALUETYPE(ProductsAndServicesCharacteristics.Owner) = TYPE(Catalog.ProductsAndServicesCategories)
			THEN TRUE
		ELSE FALSE
	END AS IsCategory,
	ProductsAndServicesCharacteristics.NotValid AS NotValid
INTO TtTotal
FROM
	Catalog.ProductsAndServicesCharacteristics AS ProductsAndServicesCharacteristics		
;

////////////////////////////////////////////////////////////////////////////////
SELECT
	ProductsAndServicesCharacteristicsOverridable.Ref AS Ref,
	ProductsAndServicesCharacteristicsOverridable.DataVersion AS DataVersion,
	ProductsAndServicesCharacteristicsOverridable.DeletionMark AS DeletionMark,
	ProductsAndServicesCharacteristicsOverridable.Owner AS Owner,
	ProductsAndServicesCharacteristicsOverridable.Code AS Code,
	ProductsAndServicesCharacteristicsOverridable.Description AS Description,
	ProductsAndServicesCharacteristicsOverridable.NotValid AS NotValid,
	ProductsAndServicesCharacteristicsOverridable.Predefined AS Predefined,
	ProductsAndServicesCharacteristicsOverridable.PredefinedDataName AS PredefinedDataName,
	ProductsAndServicesCharacteristicsOverridable.Presentation AS Presentation,
	TtTotal.IsCategory AS IsCategory,
	CASE
		WHEN AttachedFilesExist.HasFiles IS NULL
			THEN 0
		WHEN AttachedFilesExist.HasFiles
			THEN 1
		ELSE 0
	END AS HasFiles
{SELECT
	Ref.*,
	DataVersion,
	DeletionMark,
	Owner.*,
	Code,
	Description,
	NotValid,
	Predefined,
	PredefinedDataName,
	IsCategory,
	HasFiles}
FROM
	Catalog.ProductsAndServicesCharacteristics AS ProductsAndServicesCharacteristicsOverridable
		LEFT JOIN TtTotal AS TtTotal
		ON ProductsAndServicesCharacteristicsOverridable.Ref = TtTotal.Ref
		{LEFT JOIN InformationRegister.AttachedFilesExist AS AttachedFilesExist
		ON ProductsAndServicesCharacteristicsOverridable.Ref = AttachedFilesExist.ObjectWithFiles}
{WHERE
	ProductsAndServicesCharacteristicsOverridable.Ref.*,
	ProductsAndServicesCharacteristicsOverridable.DataVersion,
	ProductsAndServicesCharacteristicsOverridable.DeletionMark,
	ProductsAndServicesCharacteristicsOverridable.Owner.*,
	ProductsAndServicesCharacteristicsOverridable.Code,
	ProductsAndServicesCharacteristicsOverridable.Description,
	ProductsAndServicesCharacteristicsOverridable.NotValid,
	ProductsAndServicesCharacteristicsOverridable.Predefined,
	ProductsAndServicesCharacteristicsOverridable.PredefinedDataName,
	TtTotal.IsCategory,
	(CASE
			WHEN AttachedFilesExist.HasFiles IS NULL
				THEN 0
			WHEN AttachedFilesExist.HasFiles
				THEN 1
			ELSE 0
		END) AS HasFiles}</queryText>
