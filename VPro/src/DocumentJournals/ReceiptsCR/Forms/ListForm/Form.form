      <segments>ReceiptsCR.CashCRSession</segments>
    </notDefaultUseAlwaysAttributes>
    <notDefaultUseAlwaysAttributes xsi:type="form:DataPath">
      <segments>ReceiptsCR.CashCRUseWithoutEquipmentConnection</segments>
    </notDefaultUseAlwaysAttributes>
    <notDefaultUseAlwaysAttributes xsi:type="form:DataPath">
      <segments>ReceiptsCR.ReceiptCRNumber</segments>
    </notDefaultUseAlwaysAttributes>
    <notDefaultUseAlwaysAttributes xsi:type="form:DataPath">
      <segments>ReceiptsCR.Ref</segments>
    </notDefaultUseAlwaysAttributes>
    <notDefaultUseAlwaysAttributes xsi:type="form:DataPath">
      <segments>ReceiptsCR.ThereIsBillForReturn</segments>
    </notDefaultUseAlwaysAttributes>
    <notDefaultUseAlwaysAttributes xsi:type="form:DataPath">
      <segments>ReceiptsCR.Type</segments>
    </notDefaultUseAlwaysAttributes>
    <main>true</main>
    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>SELECT
	DocumentJournalReceiptsCR.Ref,
	DocumentJournalReceiptsCR.DeletionMark,
	DocumentJournalReceiptsCR.Archival,
	CASE
		WHEN DocumentJournalReceiptsCR.ReceiptCRNumber > 0
			THEN VALUE(Enum.ReceiptCRStatuses.Issued)
		ELSE DocumentJournalReceiptsCR.Status
	END AS Status,
	DocumentJournalReceiptsCR.Number,
	DocumentJournalReceiptsCR.Date,
	DocumentJournalReceiptsCR.Posted,
	DocumentJournalReceiptsCR.DocumentCurrency,
	DocumentJournalReceiptsCR.CashCR,
	DocumentJournalReceiptsCR.CashCRSession,
	DocumentJournalReceiptsCR.ReceiptCRNumber,
	DocumentJournalReceiptsCR.Company,
	DocumentJournalReceiptsCR.Warehouse,
	DocumentJournalReceiptsCR.DocumentAmount,
	CASE
		WHEN VALUETYPE(DocumentJournalReceiptsCR.Ref) = Type(Document.ReceiptCRReturn)
			THEN TRUE
		ELSE FALSE
	END AS ThereIsBillForReturn,
	DocumentJournalReceiptsCR.Type,
	CAST(DocumentJournalReceiptsCR.Comment AS String(500)) AS Comment,
	DocumentJournalReceiptsCR.CashCR.UseWithoutEquipmentConnection,
	DocumentJournalReceiptsCR.Author
	FROM
		DocumentJournal.ReceiptsCR AS DocumentJournalReceiptsCR</queryText>
