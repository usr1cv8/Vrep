    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>SELECT
	ChartOfAccountsManagerial.Ref,
	ChartOfAccountsManagerial.DeletionMark,
	ChartOfAccountsManagerial.Predefined,
	ChartOfAccountsManagerial.Parent,
	ChartOfAccountsManagerial.Code,
	ChartOfAccountsManagerial.Description,
	ChartOfAccountsManagerial.TypeOfAccount,
	ChartOfAccountsManagerial.Order AS Order,
	CASE
		WHEN ChartOfAccountsManagerial.Type = VALUE(AccountType.ActivePassive)
			THEN &quot;AP&quot;
		WHEN ChartOfAccountsManagerial.Type = VALUE(AccountType.Passive)
			THEN &quot;P&quot;
		WHEN ChartOfAccountsManagerial.Type = VALUE(AccountType.Active)
			THEN &quot;A&quot;
	END AS Kind,
	ChartOfAccountsManagerial.OffBalance AS OffBalance,
	ChartOfAccountsManagerial.Currency,
	ChartOfAccountsManagerial.Presentation
	FROM
		ChartOfAccounts.Managerial AS ChartOfAccountsManagerial</queryText>
      <mainTable>ChartOfAccounts.Managerial</mainTable>
