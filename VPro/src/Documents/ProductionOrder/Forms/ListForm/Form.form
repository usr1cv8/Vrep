  <attributes>
    <name>List</name>
    <id>1</id>
    <valueType>
      <types>DynamicList</types>
    </valueType>
    <view>
      <common>true</common>
    </view>
    <edit>
      <common>true</common>
    </edit>
    <notDefaultUseAlwaysAttributes xsi:type="form:DataPath">
      <segments>List.Ref</segments>
    </notDefaultUseAlwaysAttributes>
    <main>true</main>
    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>SELECT
	CASE
		WHEN DocumentProductionOrder.Posted
				AND DocumentProductionOrder.OrderState.OrderStatus &lt;> VALUE(Enum.OrderStatuses.Open)
			THEN CASE
					WHEN (ISNULL(OrdersForProductionOfBalanceAndTurnovers.QuantityReceipt, 0) &lt;> 0
							OR ISNULL(OrdersForProductionOfBalanceAndTurnovers.QuantityReceipt, 0) = 0)
							AND ISNULL(OrdersForProductionOfBalanceAndTurnovers.QuantityExpense, 0) = 0
						THEN CASE
								WHEN DocumentProductionOrder.OrderState.OrderStatus = VALUE(Enum.OrderStatuses.InProcess)
										AND Not DocumentProductionOrder.Closed
									THEN CASE
											WHEN DocumentProductionOrder.Finish &lt; &amp;CurrentDateSession
												THEN 6
											WHEN DocumentProductionOrder.Start &lt;= ENDOFPERIOD(&amp;CurrentDateSession, Day)
													AND DocumentProductionOrder.Finish >= &amp;CurrentDateSession
												THEN 4
											ELSE 2
										END
								ELSE 2
							END
					WHEN ISNULL(OrdersForProductionOfBalanceAndTurnovers.QuantityReceipt, 0) &lt;> 0
							AND ISNULL(OrdersForProductionOfBalanceAndTurnovers.QuantityExpense, 0) &lt;> 0
							AND ISNULL(OrdersForProductionOfBalanceAndTurnovers.QuantityClosingBalance, 0) = 0
						THEN 0
					ELSE CASE
							WHEN DocumentProductionOrder.OrderState.OrderStatus = VALUE(Enum.OrderStatuses.InProcess)
									AND Not DocumentProductionOrder.Closed
								THEN CASE
										WHEN DocumentProductionOrder.Finish &lt; &amp;CurrentDateSession
											THEN 5
										WHEN DocumentProductionOrder.Start &lt;= ENDOFPERIOD(&amp;CurrentDateSession, Day)
												AND DocumentProductionOrder.Finish >= &amp;CurrentDateSession
											THEN 3
										ELSE 1
									END
							ELSE 1
						END
				END
		ELSE 2
	END AS PictureNumber,
	CASE
		WHEN DocumentProductionOrder.Posted
				AND Not DocumentProductionOrder.Closed
				AND DocumentProductionOrder.OrderState.OrderStatus = VALUE(Enum.OrderStatuses.InProcess)
				AND DocumentProductionOrder.Finish &lt; &amp;CurrentDateSession
				AND ISNULL(OrdersForProductionOfBalanceAndTurnovers.QuantityClosingBalance, 0) > 0
			THEN TRUE
		ELSE FALSE
	END AS PastPerformance,
	CASE
		WHEN DocumentProductionOrder.Posted
				AND Not DocumentProductionOrder.Closed
				AND DocumentProductionOrder.OrderState.OrderStatus = VALUE(Enum.OrderStatuses.InProcess)
				AND DocumentProductionOrder.Start &lt;= ENDOFPERIOD(&amp;CurrentDateSession, Day)
				AND DocumentProductionOrder.Finish >= &amp;CurrentDateSession
				AND ISNULL(OrdersForProductionOfBalanceAndTurnovers.QuantityClosingBalance, 0) > 0
			THEN TRUE
		ELSE FALSE
	END AS ForToday,
	CASE
		WHEN DocumentProductionOrder.Posted
				AND Not DocumentProductionOrder.Closed
				AND DocumentProductionOrder.OrderState.OrderStatus = VALUE(Enum.OrderStatuses.InProcess)
			THEN TRUE
		ELSE FALSE
	END AS OrderInProcess,
	DocumentProductionOrder.Ref,
	DocumentProductionOrder.DeletionMark,
	DocumentProductionOrder.Number,
	DocumentProductionOrder.Date,
	DocumentProductionOrder.Posted,
	DocumentProductionOrder.OperationKind,
	DocumentProductionOrder.Company,
	DocumentProductionOrder.CustomerOrder,
	DocumentProductionOrder.Start,
	DocumentProductionOrder.Finish,
	DocumentProductionOrder.StructuralUnit,
	DocumentProductionOrder.Comment,
	DocumentProductionOrder.StructuralUnitReserve,
	DocumentProductionOrder.BasisDocument,
	DocumentProductionOrder.OrderState,
	DocumentProductionOrder.OrderState.OrderStatus AS OrderStateState,
	CASE
		WHEN (DocumentProductionOrder.OrderState.OrderStatus = VALUE(Enum.OrderStatuses.Open)
				OR DocumentProductionOrder.OrderState.OrderStatus = VALUE(Enum.OrderStatuses.InProcess))
				AND Not DocumentProductionOrder.Closed
			THEN &quot;In process&quot;
		ELSE CASE
				WHEN DocumentProductionOrder.OrderState.OrderStatus = VALUE(Enum.OrderStatuses.Completed)
					THEN &quot;Completed&quot;
				ELSE &quot;Canceled&quot;
			END
	END AS OrderStatus,
	DocumentProductionOrder.Closed,
	DocumentProductionOrder.Responsible,
	DocumentProductionOrder.Author,
	DocumentProductionOrder.ProductsAndServicesList,
	DocumentProductionOrder.ResourcesList,
	DocumentProductionOrder.PointInTime
FROM
	Document.ProductionOrder AS DocumentProductionOrder
		LEFT JOIN AccumulationRegister.ProductionOrders.BalanceAndTurnovers AS OrdersForProductionOfBalanceAndTurnovers
			ON DocumentProductionOrder.Ref = OrdersForProductionOfBalanceAndTurnovers.ProductionOrder</queryText>
