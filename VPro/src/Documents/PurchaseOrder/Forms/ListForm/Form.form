  <attributes>
    <name>List</name>
    <id>1</id>
    <valueType>
      <types>DynamicList</types>
    </valueType>
    <view>
      <common>true</common>
    </view>
    <edit>
      <common>true</common>
    </edit>
    <notDefaultUseAlwaysAttributes xsi:type="form:DataPath">
      <segments>List.Closed</segments>
    </notDefaultUseAlwaysAttributes>
    <notDefaultUseAlwaysAttributes xsi:type="form:DataPath">
      <segments>List.ContactPerson</segments>
    </notDefaultUseAlwaysAttributes>
    <notDefaultUseAlwaysAttributes xsi:type="form:DataPath">
      <segments>List.Counterparty</segments>
    </notDefaultUseAlwaysAttributes>
    <notDefaultUseAlwaysAttributes xsi:type="form:DataPath">
      <segments>List.OrderState</segments>
    </notDefaultUseAlwaysAttributes>
    <notDefaultUseAlwaysAttributes xsi:type="form:DataPath">
      <segments>List.OrderStatus</segments>
    </notDefaultUseAlwaysAttributes>
    <notDefaultUseAlwaysAttributes xsi:type="form:DataPath">
      <segments>List.Ref</segments>
    </notDefaultUseAlwaysAttributes>
    <main>true</main>
    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>SELECT
	CASE
		WHEN DocumentPurchaseOrder.Posted
				AND DocumentPurchaseOrder.OrderState.OrderStatus &lt;> VALUE(Enum.OrderStatuses.Open)
			THEN CASE
					WHEN RunSchedule.Order IS NULL 
							AND TimeSchedulePlan.Order IS NULL 
						THEN CASE
								WHEN DocumentPurchaseOrder.OrderState.OrderStatus = VALUE(Enum.OrderStatuses.InProcess)
										AND DocumentPurchaseOrder.Closed
									THEN 2
								ELSE 0
							END
					ELSE CASE
							WHEN ISNULL(RunSchedule.Completed, 0) = 0
									AND ISNULL(TimeSchedulePlan.Completed, 0) = 0
								THEN CASE
										WHEN DocumentPurchaseOrder.OrderState.OrderStatus = VALUE(Enum.OrderStatuses.InProcess)
												AND Not DocumentPurchaseOrder.Closed
											THEN CASE
													WHEN Not RunSchedule.Order IS NULL 
															AND RunSchedule.Period &lt; &amp;CurrentDateSession
														THEN 6
													WHEN Not RunSchedule.Order IS NULL 
															AND RunSchedule.Period = &amp;CurrentDateSession
														THEN 4
													ELSE 2
												END
										ELSE 2
									END
							ELSE CASE
									WHEN DocumentPurchaseOrder.OrderState.OrderStatus = VALUE(Enum.OrderStatuses.InProcess)
											AND Not DocumentPurchaseOrder.Closed
										THEN CASE
												WHEN Not RunSchedule.Order IS NULL 
														AND RunSchedule.Period &lt; &amp;CurrentDateSession
													THEN 5
												WHEN Not RunSchedule.Order IS NULL 
														AND RunSchedule.Period = &amp;CurrentDateSession
													THEN 3
												ELSE 1
											END
									ELSE 1
								END
						END
				END
		ELSE 2
	END AS ShipmentPictureNumber,
	CASE
		WHEN DocumentPurchaseOrder.Posted
			THEN CASE
					WHEN (DocumentPurchaseOrder.OrderState.OrderStatus = VALUE(Enum.OrderStatuses.Open)
							OR ISNULL(PaymentEvent.Amount, 0) &lt;> 0)
							AND ISNULL(PaymentEvent.PaymentAmount, 0) + ISNULL(PaymentEvent.AdvanceAmount, 0) = 0
						THEN CASE
								WHEN DocumentPurchaseOrder.SchedulePayment
										AND DocumentPurchaseOrder.OrderState.OrderStatus = VALUE(Enum.OrderStatuses.InProcess)
										AND Not DocumentPurchaseOrder.Closed
										AND Not PaymentSchedule.InvoiceForPayment IS NULL 
									THEN CASE
											WHEN PaymentSchedule.Period &lt; &amp;CurrentDateSession
												THEN 6
											WHEN PaymentSchedule.Period = &amp;CurrentDateSession
												THEN 4
											ELSE 2
										END
								ELSE 2
							END
					WHEN ISNULL(PaymentEvent.Amount, 0) &lt;= ISNULL(PaymentEvent.PaymentAmount, 0) + ISNULL(PaymentEvent.AdvanceAmount, 0)
						THEN CASE
								WHEN DocumentPurchaseOrder.OrderState.OrderStatus = VALUE(Enum.OrderStatuses.InProcess)
										AND DocumentPurchaseOrder.Closed
									THEN 2
								ELSE 0
							END
					ELSE CASE
							WHEN DocumentPurchaseOrder.SchedulePayment
									AND DocumentPurchaseOrder.OrderState.OrderStatus = VALUE(Enum.OrderStatuses.InProcess)
									AND Not DocumentPurchaseOrder.Closed
									AND Not PaymentSchedule.InvoiceForPayment IS NULL 
								THEN CASE
										WHEN PaymentSchedule.Period &lt; &amp;CurrentDateSession
											THEN 5
										WHEN PaymentSchedule.Period = &amp;CurrentDateSession
											THEN 3
										ELSE 1
									END
							ELSE 1
						END
				END
		ELSE 2
	END AS PaymentPictureNumber,
	CASE
		WHEN DocumentPurchaseOrder.Posted
				AND DocumentPurchaseOrder.DocumentAmount > 0
			THEN CAST((ISNULL(PaymentEvent.PaymentAmount, 0) + ISNULL(PaymentEvent.AdvanceAmount, 0)) * 100 / DocumentPurchaseOrder.DocumentAmount AS NUMBER(15, 2))
		ELSE 0
	END AS PaymentPercentage,
	CASE
		WHEN DocumentPurchaseOrder.Posted
				AND Not DocumentPurchaseOrder.Closed
				AND DocumentPurchaseOrder.OrderState.OrderStatus = VALUE(Enum.OrderStatuses.InProcess)
				AND Not RunSchedule.Order IS NULL 
				AND RunSchedule.Period &lt; &amp;CurrentDateSession
			THEN TRUE
		ELSE FALSE
	END AS PastPerformance,
	CASE
		WHEN DocumentPurchaseOrder.Posted
				AND Not DocumentPurchaseOrder.Closed
				AND DocumentPurchaseOrder.SchedulePayment
				AND DocumentPurchaseOrder.OrderState.OrderStatus = VALUE(Enum.OrderStatuses.InProcess)
				AND Not PaymentSchedule.InvoiceForPayment IS NULL 
				AND PaymentSchedule.Period &lt; &amp;CurrentDateSession
			THEN TRUE
		ELSE FALSE
	END AS OverduePayment,
	CASE
		WHEN DocumentPurchaseOrder.Posted
				AND Not DocumentPurchaseOrder.Closed
				AND DocumentPurchaseOrder.OrderState.OrderStatus = VALUE(Enum.OrderStatuses.InProcess)
				AND Not RunSchedule.Order IS NULL 
				AND RunSchedule.Period = &amp;CurrentDateSession
			THEN TRUE
		WHEN DocumentPurchaseOrder.Posted
				AND Not DocumentPurchaseOrder.Closed
				AND DocumentPurchaseOrder.SchedulePayment
				AND DocumentPurchaseOrder.OrderState.OrderStatus = VALUE(Enum.OrderStatuses.InProcess)
				AND Not PaymentSchedule.InvoiceForPayment IS NULL 
				AND PaymentSchedule.Period = &amp;CurrentDateSession
			THEN TRUE
		ELSE FALSE
	END AS ForToday,
	CASE
		WHEN DocumentPurchaseOrder.Posted
				AND Not DocumentPurchaseOrder.Closed
				AND DocumentPurchaseOrder.OrderState.OrderStatus = VALUE(Enum.OrderStatuses.InProcess)
			THEN TRUE
		ELSE FALSE
	END AS OrderInProcess,
	DocumentPurchaseOrder.Ref,
	DocumentPurchaseOrder.DeletionMark,
	DocumentPurchaseOrder.Number,
	DocumentPurchaseOrder.Date,
	DocumentPurchaseOrder.Posted,
	DocumentPurchaseOrder.Company,
	DocumentPurchaseOrder.OperationKind,
	DocumentPurchaseOrder.Counterparty,
	DocumentPurchaseOrder.Counterparty.ContactPerson AS ContactPerson,
	DocumentPurchaseOrder.Contract,
	DocumentPurchaseOrder.ReceiptDate AS ReceiptDate,
	DocumentPurchaseOrder.DocumentCurrency,
	DocumentPurchaseOrder.AmountIncludesVAT,
	DocumentPurchaseOrder.IncludeVATInPrice,
	DocumentPurchaseOrder.ExchangeRate,
	DocumentPurchaseOrder.Multiplicity,
	DocumentPurchaseOrder.StructuralUnitReserve,
	DocumentPurchaseOrder.CustomerOrder,
	DocumentPurchaseOrder.DocumentAmount,
	DocumentPurchaseOrder.CounterpartyPriceKind,
	DocumentPurchaseOrder.Event,
	DocumentPurchaseOrder.Responsible,
	DocumentPurchaseOrder.StructuralUnit,
	DocumentPurchaseOrder.Comment,
	DocumentPurchaseOrder.SchedulePayment,
	DocumentPurchaseOrder.PettyCash,
	DocumentPurchaseOrder.BankAccount,
	DocumentPurchaseOrder.CashAssetsType,
	DocumentPurchaseOrder.OrderState,
	DocumentPurchaseOrder.OrderState.OrderStatus AS OrderStateState,
	CASE
		WHEN (DocumentPurchaseOrder.OrderState.OrderStatus = VALUE(Enum.OrderStatuses.Open)
				OR DocumentPurchaseOrder.OrderState.OrderStatus = VALUE(Enum.OrderStatuses.InProcess))
				AND Not DocumentPurchaseOrder.Closed
			THEN &quot;In process&quot;
		ELSE CASE
				WHEN DocumentPurchaseOrder.OrderState.OrderStatus = VALUE(Enum.OrderStatuses.Completed)
					THEN &quot;Completed&quot;
				ELSE &quot;Canceled&quot;
			END
	END AS OrderStatus,
	DocumentPurchaseOrder.Closed,
	DocumentPurchaseOrder.IncomingDocumentNumber,
	DocumentPurchaseOrder.IncomingDocumentDate,
	DocumentPurchaseOrder.Author,
	DocumentPurchaseOrder.PointInTime,
	ISNULL(PaymentEvent.Amount, 0) - ISNULL(PaymentEvent.PaymentAmount, 0) - ISNULL(PaymentEvent.AdvanceAmount, 0) AS ForPayment,
	ISNULL(PaymentEvent.PaymentAmount, 0) + ISNULL(PaymentEvent.AdvanceAmount, 0) AS PaymentAmountTurnover
	FROM
	Document.PurchaseOrder AS DocumentPurchaseOrder
		LEFT JOIN InformationRegister.OrderFulfillmentSchedule AS RunSchedule
		ON DocumentPurchaseOrder.Ref = RunSchedule.Order
			AND (RunSchedule.Period &lt;= &amp;CurrentDateSession)
		LEFT JOIN InformationRegister.OrderFulfillmentSchedule AS TimeSchedulePlan
		ON DocumentPurchaseOrder.Ref = TimeSchedulePlan.Order
			AND (TimeSchedulePlan.Period > &amp;CurrentDateSession)
		{LEFT JOIN InformationRegister.OrdersPaymentSchedule AS PaymentSchedule
		ON DocumentPurchaseOrder.Ref = PaymentSchedule.InvoiceForPayment
			AND (PaymentSchedule.Period &lt;= &amp;CurrentDateSession)}
		LEFT JOIN InformationRegister.OrdersPaymentFact AS PaymentEvent
			ON DocumentPurchaseOrder.Ref = PaymentEvent.InvoiceForPayment</queryText>
      <mainTable>Document.PurchaseOrder</mainTable>
      <dynamicDataRead>true</dynamicDataRead>
